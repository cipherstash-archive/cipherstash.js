{
  "name": "@cipherstash/stashjs",
  "version": "0.2.24",
  "description": "Datastore & Search API for CipherStash",
  "main": "dist/index.js",
  "scripts": {
    "build": "./scripts/pkg-build.sh",
    "build-then-watch": "pnpm build && pnpx tsc --watch",
    "prepublishOnly": "pnpm build --filter ./",
    "postinstall": "./scripts/pkg-postinstall.sh",
    "clean": "rm -fr ./dist ./build",
    "test": "pnpx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipherstash/stashjs.git"
  },
  "keywords": [
    "cipherstash",
    "encryption",
    "database",
    "search",
    "full text search",
    "cryptography",
    "ORE",
    "order revealing encryption",
    "security",
    "secure",
    "client-side encryption",
    "stashjs",
    "stash"
  ],
  "contributors": [
    "Dan Draper <dan@cipherstash.com>",
    "James Sadler <james@cipherstash.com>",
    "Lindsay Holmwood <lindsay@cipherstash.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipherstash/stashjs/issues"
  },
  "homepage": "https://github.com/cipherstash/stashjs#readme",
  "devDependencies": {
    "@aws-sdk/types": "^3.15.0",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bson": "^4.0.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "node-addon-api": "^3.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^2.2.0",
    "@cipherstash/stashjs-grpc": "workspace:*",
    "@grpc/grpc-js": "^1.3.2",
    "@grpc/proto-loader": "^0.6.2",
    "@types/stringify-object": "^3.3.1",
    "aws-sdk": "^2.918.0",
    "bson": "^4.4.0",
    "is-plain-object": "^5.0.0",
    "node-gyp": "^8.0.0",
    "siphash": "^1.1.0",
    "stringify-object": "^3.3.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "node",
      "d.ts",
      "json"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "files": [
    "scripts/*",
    "src/**",
    "src/crypto/ore/fastore/**",
    "dist/**",
    "babel.config.js",
    "binding.gyp",
    "tsconfig.json"
  ]
}
