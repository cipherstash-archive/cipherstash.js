{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "inlineSourceMap": false,
    "inlineSources": false,
    "noPropertyAccessFromIndexSignature": false,
    "noImplicitAny": false,
    "outDir": "./build",
    "rootDir": "./src",
    "tsBuildInfoFile": "./build/tsconfig.tsbuildinfo"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
