{
  "name": "@cipherstash/ore-rs",
  "version": "0.3.0",
  "description": "Node bindings for ore.rs",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics && npx tsc",
    "build-release": "npx cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics --release",
    "prepublishOnly": "npm run build-release && npx tsc",
    "install": "npm run build-release",
    "test": "cargo test && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipherstash/node-ore-rs.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    "Dan Draper <dan@cipherstash.com>",
    "James Sadler <james@cipherstash.com>"
  ],
  "homepage": "https://cipherstash.com",
  "keywords": [
    "cipherstash",
    "encryption",
    "database",
    "search",
    "full text search",
    "cryptography",
    "ORE",
    "order revealing encryption",
    "security",
    "secure",
    "client-side encryption",
    "stashjs",
    "stash"
  ],
  "license": "GPLv3",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "cargo-cp-artifact": "^0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "node",
      "d.ts",
      "json"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "files": [
    "babel.config.js",
    "Cargo.lock",
    "Cargo.toml",
    "COPYING",
    "dist/**",
    "native",
    "scripts/*",
    "src/**",
    "tsconfig.json"
  ]
}
