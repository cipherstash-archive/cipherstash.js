{
  "name": "@cipherstash/stash-rs",
  "version": "0.3.1",
  "description": "Node bindings for the CipherStash Rust client.",
  "main": "dist/index.js",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf index.node dist/",
    "build": "rm -f index.node && npx cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics && tsc --build",
    "build-release": "rm -f index.node && npx cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics --release",
    "prepublishOnly": "npm run build-release && tsc --build",
    "install": "npm run build-release",
    "test": "cargo test && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipherstash/node-stash-rs.git"
  },
  "contributors": [
    "Dan Draper <dan@cipherstash.com>",
    "James Sadler <james@cipherstash.com>"
  ],
  "homepage": "https://cipherstash.com",
  "keywords": [
    "cipherstash",
    "encryption",
    "database",
    "search",
    "full text search",
    "cryptography",
    "ORE",
    "order revealing encryption",
    "security",
    "secure",
    "client-side encryption",
    "stashjs",
    "stash"
  ],
  "license": "GPLv3",
  "dependencies": {
    "cargo-cp-artifact": "^0.1",
    "cbor": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "node",
      "d.ts",
      "json"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "files": [
    "babel.config.js",
    "Cargo.lock",
    "Cargo.toml",
    "COPYING",
    "dist/**",
    "native",
    "scripts/*",
    "src/**",
    "tsconfig.json"
  ]
}
