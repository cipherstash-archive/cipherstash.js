{
  "name": "@cipherstash/stash-rs",
  "version": "0.5.0",
  "description": "Node bindings for the CipherStash Rust client.",
  "main": "dist/index.js",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rm -rf ./pkg",
    "build": "pnpm rebuild wasm-pack && rm -rf ./pkg && wasm-pack build . --target nodejs && tsc --build && ./fix-imports.sh",
    "prepublishOnly": "npm run build",
    "test": "cargo test && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cipherstash/cipherstash.js.git",
    "directory": "packages/stash-rs"
  },
  "contributors": [
    "Dan Draper <dan@cipherstash.com>",
    "James Sadler <james@cipherstash.com>"
  ],
  "homepage": "https://cipherstash.com",
  "keywords": [
    "cipherstash",
    "encryption",
    "database",
    "search",
    "full text search",
    "cryptography",
    "ORE",
    "order revealing encryption",
    "security",
    "secure",
    "client-side encryption",
    "stashjs",
    "stash"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "cargo-cp-artifact": "^0.1",
    "cbor": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wasm-pack": "^0.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "node",
      "d.ts",
      "json"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "files": [
    "dist/**",
    "pkg/**"
  ]
}
